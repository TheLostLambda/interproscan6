params {
    appsConfig {
        antifam {
            name = "AntiFam"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "antifam/AntiFam.hmm"
        }
        cdd {
            name = "CDD"
            runner = "rpsblast"
            rpsblast_db = "cdd/db/Cdd_NCBI"
            rpsproc_db = "cdd/data"
        }
        coils {
            name = "COILS"
            runner = "ncoils"
        }
        cathgene3d {
            name = "CATH-Gene3D"
            aliases = ["gene3d"]
            invalid_chars = "-."
            runner = "hmmer"
            hmm = "cath/gene3d/gene3d_main.hmm"
            model2sfs = "cath/gene3d/model_to_family_map.tsv"
            disc_regs = "cath/gene3d/discontinuous_regs.pkl.py3"
        }
        cathfunfam {
            name = "CATH-FunFam"
            aliases = ["funfam"]
            invalid_chars = "-."
            runner = "hmmer"
            dir = "cath/funfam/"
            chunkSize = 1000
        }
        hamap {
            name = "HAMAP"
            invalid_chars = "-."
            runner = "hmmer"
            hmm = "hamap/hamap.hmm.lib"
            dir = "hamap/profiles"
        }
        mobidblite {
            name = "MobiDB-lite"
            runner = "idrpred"
        }
        ncbifam {
            name = "NCBIFAM"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "ncbifam/ncbifam.hmm"
        }
        panther {
            name = "PANTHER"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "panther/famhmm/binHmm"
            msf = "panther/Tree_MSF"
            paint = "panther/PAINT_Annotations"
        }
        phobius {
            name = "Phobius"
            invalid_chars = "-*.OXUZJ"
            dir = ""
        }
        pfam {
            name = "Pfam"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "pfam/pfam_a.hmm"
            switches = "-Z 61295632 --cut_ga --cpu 1"
            postprocess {
                min_length = 8
                seed = "pfam/pfam_a.seed"
                clan = "pfam/pfam_clans"
                data = "pfam/pfam_a.dat"
            }
        }
        pirsf {
            name = "PIRSF"
            invalid_chars = "-"
            runner = "hmmer"
            hmm = "pirsf/sf_hmm_all"
            switches = "-E 0.01 --acc --cpu 1"
            postprocess {
                data = "pirsf/pirsf.dat"
            }
        }
        pirsr {
            name = "PIRSR"
            invalid_chars = "-."
            hmm = "pirsr/sr_hmm_all"
            rules = "pirsr/sr_uru.json"
        }
        prints {
            name = "PRINTS"
            runner = "fingerPRINTScan"
            bin = "$projectDir/bin/prints/fingerPRINTScan"
            data {
                kdat = "prints/prints.kdat"
                pval = "prints/prints.pval"
                hierarchy = "prints/FingerPRINTShierarchy.db"
            }
            switches = "-e 0.0001 -d 10 -E 257043 84355444 -fj -o 15"
        }
        prositepatterns {
            name = "PROSITE patterns"
            runner = "prosite_patterns"
            switches = "-r -s -o ipro"
            data = "prosite/prosite_patterns.dat"
            evaluator = "prosite/evaluator.dat"
        }
        prositeprofiles {
            name = "PROSITE profiles"
            invalid_chars = "-."
            runner = "prosite_profiles"
            switches = "-f -o 7 -t 4"
            data = "prosite/prosite_profiles"
            skip_flagged_profiles = "prosite/skip_flagged_profiles.txt"
        }
        sfld {
            name = "SFLD"
            invalid_chars = "-."
            runner = "hmmer"
            hmm = "sfld/sfld.hmm"
            switches = "-Z 378 --acc --cut_ga --cpu 1"
            postprocess {
                sites_annotation = "sfld/sfld_sites.annot"
                hierarchy = "sfld/sfld_hierarchy_flat.txt"
            }
        }
        smart {
            name = "SMART"
            runner = "hmmer"
            hmm = "smart/smart.HMMs"
            hmmbin = "smart/smart.HMMs.bin"
        }
        superfamily {
            name = "SUPERFAMILY"
            invalid_chars = "-"
            hmm = "superfamily/hmmlib_1.75"
            selfhits = "superfamily/self_hits.tab"
            cla = "superfamily/dir.cla.scop.txt_1.75"
            model = "superfamily/model.tab"
            pdbj95d = "superfamily/pdbj95d"
        }
        signalp {
            name = "SignalP"
            release = "6.0h"
            runner = "signalp"
            data {
                organism = "other"
                pvalue = 0.95
            }
        }
        signalp_euk {
            name = "SignalP-Euk"
            release = "4.1"
            runner = "signalp_euk"
            data {
                organism = "euk"
                pvalue = 0.95
            }
        }
        tmhmm {
            name = "TMHMM"
            release = "2.0c"
            runner = "tmhmm"
            bin = "$projectDir/bin/tmhmm/decodeanhmm"
            data {
                model = "tmhmm/TMHMM.model"
            }
            switches = "-N 1 -PrintNumbers"
        }
    }
}

